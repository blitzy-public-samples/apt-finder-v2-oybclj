# Specifies the environment in which the application is running
NODE_ENV=development

# The port on which the API server will run
PORT=3000

# Connection string for the PostgreSQL database
DATABASE_URL=postgresql://username:password@localhost:5432/apartment_finder

# Connection string for the Redis cache
REDIS_URL=redis://localhost:6379

# Secret key for JWT token generation and verification
JWT_SECRET=your_jwt_secret_key_here

# API key for accessing Zillow's API
ZILLOW_API_KEY=your_zillow_api_key_here

# Client ID for PayPal API integration
PAYPAL_CLIENT_ID=your_paypal_client_id_here

# Client Secret for PayPal API integration
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# API key for SendGrid email service
SENDGRID_API_KEY=your_sendgrid_api_key_here

# AWS access key ID for S3 bucket access
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here

# AWS secret access key for S3 bucket access
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# Name of the S3 bucket for file storage
S3_BUCKET_NAME=apartment-finder-files

# API key for Google Maps integration
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Logging level for the application
LOG_LEVEL=info

